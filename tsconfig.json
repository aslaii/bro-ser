{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@assets/*": [
        "./src/assets/*"
      ],
      "@app/*": [
        "./src/app/*"
      ],
      "@config/*": [
        "./src/app/config/*"
      ],
      "@components/*": [
        "./src/app/components/*"
      ],
      "@reducers/*": [
        "./src/app/reducers/*"
      ],
      "@services/*": [
        "./src/app/services/*"
      ],
      "@shared/*": [
        "./src/app/shared/*"
      ],
      "@locale/*": [
        "./src/locale/*"
      ],
      "~/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "metro.config.js",
    "nativewind-env.d.ts"
  ]
}
